void setup()
{
  pinMode(buttonPin, INPUT);
  pinMode(ledPin, OUTPUT);
  Serial.begin(9600);
}

void loop()
{
  buttonState = digitalRead(buttonPin);
  
  if (buttonState != lastButtonState){
    if (buttonState == HIGH) {
      Serial.println("step down");
      triggerVoice = 2;
    } else {
      Serial.println("step up");
      triggerVoice = 1;  
    }
    // Delay for debouncing
    delay(50);  
  }
  
  if (triggerVoice == 2) {
      //Serial.println(buttonState);
      startPlayback(stepDown, sizeof(stepDown));
      delay(800);
  }
  
  if (triggerVoice == 1) {
      //Serial.println("ojojo");
      startPlayback(stepUp, sizeof(stepUp));
      delay(800);
  }

  stopPlayback();
  triggerVoice = 0;
  lastButtonState = buttonState;
}

